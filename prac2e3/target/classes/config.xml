<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="engineBean" class="vesit.prathamesh.student.prac2e3.Engine">
        <property name="chasisNumber" value="A001"/>
        <property name="gears" value="5"/>
    </bean>
    <bean id ="carBean" class="vesit.prathamesh.student.prac2e3.Car" autowire="constructor">
        <property name="carColor" value="blue"/>
        <property name="brand" value="Maruti"/>
    </bean>
</beans>